{
  "manifest_version": 2,

  "name": "background-red",
  "description": "This extension will execute inject.js in the background.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["index.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
